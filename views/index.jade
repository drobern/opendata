    function drawPie() {

      var chart = new google.visualization.DataTable(pd);

      var options = {
                  title: 'Food Groups Distribution',
                  is3D: true,
      };
      
      if (pd) {
        var table = new google.visualization.PieChart(document.getElementById('pie_div'));
        table.draw(chart, options);
      }

    }
    
