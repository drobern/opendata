    function drawTableV() {
      var chart = new google.visualization.DataTable(vd); 
      
      if (vd) {
        var table = new google.visualization.Table(document.getElementById('chartV_div'));
        table.draw(chart, {showRowNumber: false, width: '100%', page:'enable', pageSize: 10, sortColumn: 0, sortAscending: true});
      }
    }

      

    function drawTableG() {
      var chart = new google.visualization.DataTable(gd);

